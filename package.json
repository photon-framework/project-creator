{
  "name": "photon-app",
  "repository": "https://github.com/Frank-Mayer/photon-app.git",
  "license": "MIT",
  "scripts": {
    "dev": "photon \"./src/\" --serve 12345 --open",
    "clean": "yarn clean-windows || yarn clean-linux",
    "clean-linux": "rm -rf \"./.parcel-cache\" && rm -rf \"./dist\"",
    "clean-windows": "rmdir /Q/S \"./.parcel-cache\" && rmdir /Q/S \"./dist\"",
    "build": "photon \"./src/\""
  },
  "dependencies": {
    "@frank-mayer/magic": "^4.0.0",
    "@zip.js/zip.js": "^2.5.17",
    "downloadjs": "^1.4.7",
    "photon-re": "^1.1.2"
  },
  "devDependencies": {
    "@types/downloadjs": "^1.4.3",
    "photon-cli": "^1.1.0",
    "typescript": "^4.7.4"
  }
}
